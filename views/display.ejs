<%- include('./header.ejs') %>

    <div class="container mt-5">
        <div class="row">
            <div class="col-md-4"><h1>Display</h1></div>
        </div>
        <div class="row">
            <!--blog table-->
            <table class="table table-bordered table-striped">
                <thead style="background-color:rgb(43, 205, 226);">
                <tr>
                    <th scope="col">Id No.</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Number</th>
                    <th scope="col">DOB</th>
                    <th scope="col">Gender</th>
                    <th scope="col">Address</th>
                    <th scope="col">College</th>
                    <th scope="col">Course</th>
                    <th scope="col">Branch</th>
                </tr>
                
                </thead>
                <tbody>

                <% df.forEach((item)=>{ %>
                <tr>
                    <th scope="row"><%= item._id %></th>
                    <td><%= item.name %></td>
                    <td><%= item.email %></td>
                    <td><%= item.number %></td>
                    <td><%= item.dob %></td>
                    <td><%= item.gender %></td>
                    <td><%= item.address %></td>
                    <td><%= item.college %></td>
                    <td><%= item.course %></td>
                    <td><%= item.branch %></td>
                </tr>                          
                <% }) %>

                </tbody>
            </table>
        </div>
        <div class="row"><div class="col-md-12"><center><a href="/dashboard"><button>Go To Dashboard</button></a></center></div></div>
    </div>

<%- include('./footer.ejs')%>