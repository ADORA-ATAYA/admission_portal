                    <% if (locals.item) { %>
                        <li><a href="#" class="nav-link" ><img class="userimg" src="<%= item.image.url %>"></a></li>
                    <% } else { %>
                        <li><a href="#" class="nav-link" ><img class="userimg" src="/images/course_enroll.jpg"></a></li>
                    <% } %>
                    
                </ul>
